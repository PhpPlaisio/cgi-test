{
  "name": "plaisio/cgi-test",
  "description": "PhpPlaisio: CGI",
  "keywords": [
    "PhpPlaisio",
    "CGI"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/cgi": "^1.3.0",
    "plaisio/exception": "^1.2.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/obfuscator": "^2.1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "plaisio/console-kernel": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Cgi\\Test\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
